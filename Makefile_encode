obj = IOFile.o IOMixer.o IODsp.o Encode.o mainencode.o

encode : $(obj) 
	gcc -L/usr/local/lib -fexpensive-optimizations -funroll-loops -finline-functions  -ffast-math -Wall -g -O2  -o encode -lm -export-dynamic $(obj)
IOFile.o:IOFile.c IOFile.h
	gcc -c IOFile.c
IOMixer.o:IOMixer.c IOMixer.h IOFile.h
	gcc -c IOMixer.c
IODsp.o:IODsp.c IODsp.h IOFile.h
	gcc -c IODsp.c
Encode.o:Encode.c Encode.h IODsp.h IOMixer.h
	gcc -I/usr/local/include -c Encode.c
mainencode.o:mainencode.c Encode.h IOFile.h
	gcc -c mainencode.c
