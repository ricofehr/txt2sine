obj = IOFile.o IOMixer.o IODsp.o Decode.o maindecode.o

encode : $(obj) 
	gcc -L/usr/local/lib -fexpensive-optimizations -funroll-loops -finline-functions  -ffast-math -Wall -g -O2  -o bin/decode -export-dynamic $(obj) -lm -lfftw3
IOFile.o:IOFile.c IOFile.h
	gcc -c IOFile.c
IOMixer.o:IOMixer.c IOMixer.h IOFile.h
	gcc -c IOMixer.c
IODsp.o:IODsp.c IODsp.h IOFile.h
	gcc -c IODsp.c
Decode.o:Decode.c Decode.h IODsp.h IOMixer.h
	gcc -I/usr/local/include -c Decode.c
maindecode.o:maindecode.c Decode.h IOFile.h
	gcc -c maindecode.c
